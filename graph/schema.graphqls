# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type Mutation {
  loginGoogle(
    token: String!
    name: String!
    email: String!
    avatar: String!
  ): Auth!
  logout: User!
  logoutAll: User!
}

type Query {
  me: User!
}

type User {
  name: String!
  username: String!
  email: String!
  avatar: String!
  accountType: Int!
}

type Auth {
  user: User!
  token: String!
}
